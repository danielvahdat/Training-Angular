div(ng-controller="logoutController")
.heading
  nav.navbar.navbar-inverse
    .container-fluid
      .navbar-header
        a.navbar-brand(href='#') GeekHouse
      ul.nav.navbar-nav
        li
          a(ui-sref='home') Home
      ul.nav.navbar-nav.navbar-right
        li
          a(ui-sref='login') Login
  div(row)
    ul(class="nav nav-tabs")
      li(ng-repeat='tab in tabs' ng-show='showMenue(tab)'   active='{{tab.active}}')
        a(data-toggle="tab" ui-sref='{{tab.route}}')  {{tab.heading}}
  div(class='row Panelmargin')
    div(class='row paddingRightDiv borderdashedDetail paddingLeftDiv') 
      div(class='col-md-12 rowHeight')
        button(class='btn regbtn' ng-click='logout()') logout 

